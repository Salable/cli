{
  "name": "@salable/cli",
  "version": "1.8.0",
  "description": "A CLI to interact with your Salable Account",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Salable/cli.git",
  "bugs": {
    "url": "https://github.com/Salable/cli/issues"
  },
  "homepage": "https://github.com/Salable/cli#readme",
  "author": "Salable",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:globalCoverage": "jest --coverage --passWithNoTests",
    "test:coverage": "jest --coverage --changedSince=origin/main --passWithNoTests",
    "preversion": "npm run build"
  },
  "bin": {
    "salable": "dist/index.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/ejs": "^2.6.0",
    "@types/inquirer": "^0.0.43",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.11.9",
    "@types/prompts": "^2.4.1",
    "@types/shelljs": "^0.8.0",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^19.0.5",
    "shx": "^0.3.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^7.0.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@altostra/cli-login-auth0": "^2.0.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "inquirer": "^8.2.5",
    "isomorphic-fetch": "^3.0.0",
    "node": "^18.10.0",
    "yargs": "^17.6.0"
  }
}
